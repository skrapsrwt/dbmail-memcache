"		END;\n" \
"	END;\n" \
"CREATE TRIGGER fk_update1_subscription_users_idnr\n" \
"	BEFORE UPDATE ON dbmail_subscription\n" \
"	FOR EACH ROW BEGIN\n" \
"		SELECT CASE \n" \
"			WHEN (new.user_id IS NOT NULL)\n" \
